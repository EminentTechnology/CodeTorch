<?xml version="1.0" encoding="utf-8"?>
<Screen xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>LookupCreate.aspx</Name>
  <PageTemplate>
    <Mode>Static</Mode>
    <Name>Default</Name>
  </PageTemplate>
  <Type>Screen</Type>
  <Folder>CodeTorch</Folder>
  <RequireAuthentication>false</RequireAuthentication>
  <SupportsTransactions>true</SupportsTransactions>
  <ValidateRequest>true</ValidateRequest>
  <Menu>
    <DisplayMenu>false</DisplayMenu>
  </Menu>
  <Title>
    <Name>Lookup</Name>
    <UseCommand>false</UseCommand>
  </Title>
  <SubTitle>
    <Name>Create</Name>
    <UseCommand>false</UseCommand>
  </SubTitle>
  <ScreenPermission>
    <CheckPermission>false</CheckPermission>
  </ScreenPermission>
  <Sections>
    <Section xsi:type="AlertSection">
      <Name>Alert</Name>
      <Type>Alert</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Top</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <IncludeValidationSummary>true</IncludeValidationSummary>
    </Section>
    <Section xsi:type="EditSection">
      <Name>General Info</Name>
      <Type>Edit</Type>
      <Mode>All</Mode>
      <ContainerMode>Panel</ContainerMode>
      <Visible>true</Visible>
      <ContainerElement>fieldset</ContainerElement>
      <ContainerCssClass>form-horizontal</ContainerCssClass>
      <ContentPane>Left</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets>
        <Widget xsi:type="TextBoxControl">
          <Name>Name</Name>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-md-10</ControlContainerCssClass>
          <Label>Type</Label>
          <LabelCssClass>col-md-2</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>form-group</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>TextBox</Type>
          <IsRequired>true</IsRequired>
          <Visible>true</Visible>
          <DataField>Name</DataField>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
        </Widget>
      </Widgets>
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
    </Section>
    <Section xsi:type="ButtonListSection">
      <Name>Buttons</Name>
      <CssClass>text-center</CssClass>
      <Type>ButtonList</Type>
      <Mode>All</Mode>
      <ContainerMode>Plain</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Bottom</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Buttons>
        <Button>
          <Name>Save</Name>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <Type>Button</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <CssClass>btn btn-primary</CssClass>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
          <Text>Save</Text>
          <CausesValidation>true</CausesValidation>
          <OnClick>
            <Commands>
              <InsertUpdateSaveCommand>
                <Name>SaveCommand</Name>
                <Type>InsertUpdateSaveCommand</Type>
                <EntityID>LookupType</EntityID>
                <EntityInputType>QueryString</EntityInputType>
                <InsertCommand>Lookup_Create</InsertCommand>
                <AfterInsertConfirmationMessage>Record was added successfully</AfterInsertConfirmationMessage>
                <AfterUpdateConfirmationMessage>Record was updated successfully</AfterUpdateConfirmationMessage>
                <RedirectAfterInsert>true</RedirectAfterInsert>
                <RedirectAfterUpdate>false</RedirectAfterUpdate>
                <AfterInsertRedirectUrl>~/App/CodeTorch/LookupOverview.aspx?LookupType={0}</AfterInsertRedirectUrl>
              </InsertUpdateSaveCommand>
            </Commands>
          </OnClick>
        </Button>
        <Button>
          <Name>Cancel</Name>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <Type>Button</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <CssClass>btn btn-default</CssClass>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
          <Text>Cancel</Text>
          <CausesValidation>false</CausesValidation>
          <OnClick>
            <Commands>
              <NavigateToUrlCommand>
                <Name>NavigateOnCancel</Name>
                <Type>NavigateToUrlCommand</Type>
                <NavigateUrl>~/App/CodeTorch/LookupSearch.aspx</NavigateUrl>
                <EntityID>LookupType</EntityID>
              </NavigateToUrlCommand>
            </Commands>
          </OnClick>
        </Button>
      </Buttons>
    </Section>
  </Sections>
  <SectionZoneLayout>SingleColumn</SectionZoneLayout>
  <Settings />
  <Scripts />
  <DataCommands>
    <DataCommand>
      <Name>Lookup_Create</Name>
      <Parameters>
        <Parameter>
          <Name>Name</Name>
          <InputType>Control</InputType>
          <InputKey>Name</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
  </DataCommands>
  <OnPageLoad>
    <Commands>
      <DefaultOrPopulateScreenCommand>
        <Name>DefaultOrPopulateScreen</Name>
        <Type>DefaultOrPopulateScreenCommand</Type>
        <EntityID>LookupType</EntityID>
        <EntityInputType>QueryString</EntityInputType>
        <ExecuteOnPostBack>false</ExecuteOnPostBack>
      </DefaultOrPopulateScreenCommand>
    </Commands>
  </OnPageLoad>
  <OnPageInit>
    <Commands>
      <RenderPageSectionsCommand>
        <Name>RenderPageSections</Name>
        <Type>RenderPageSectionsCommand</Type>
        <EntityID>LookupType</EntityID>
        <EntityInputType>QueryString</EntityInputType>
        <Mode>InsertEdit</Mode>
      </RenderPageSectionsCommand>
    </Commands>
  </OnPageInit>
</Screen>