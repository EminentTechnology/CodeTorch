  <Name>SMS_Send</Name>
  <Type>Text</Type>
  <Text>
    INSERT INTO OutboundSMS
    (
    MessageID,
    PhoneNo,
    MessageText,
    Sent,
    SendDate,
    Template,
    EntityType,
    EntityID,
    ErrorMessage,
    UserName,
    Password,
    ChannelID,
    CreatedOn,
    CreatedBy,
    ModifiedOn,
    ModifiedBy
    )
    VALUES
    (
    @MessageID,
    @PhoneNo,
    @MessageText,
    0,
    null,
    @Template,
    @EntityType,
    @EntityID,
    null,
    @UserName,
    @Password,
    @ChannelID,
    @CreatedOn,
    @CreatedBy,
    @CreatedOn,
    @CreatedBy
    )
  </Text>
  <ReturnType>Integer</ReturnType>
