<?xml version="1.0" encoding="utf-8"?>
<Screen xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>Welcome.aspx</Name>
  <PageTemplate>
    <Mode>Static</Mode>
    <Name>App</Name>
  </PageTemplate>
  <Type>Screen</Type>
  <Folder>Home</Folder>
  <RequireAuthentication>true</RequireAuthentication>
  <SupportsTransactions>true</SupportsTransactions>
  <ValidateRequest>true</ValidateRequest>
  <Menu>
    <DisplayMenu>false</DisplayMenu>
  </Menu>
  <Title>
    <Name />
    <UseCommand>false</UseCommand>
    <CommandFormatString>{PageTitle}</CommandFormatString>
    <CommandName />
  </Title>
  <SubTitle>
    <UseCommand>false</UseCommand>
  </SubTitle>
  <ScreenPermission>
    <CheckPermission>false</CheckPermission>
  </ScreenPermission>
  <Sections>
    <Section xsi:type="AlertSection">
      <Name>Alert</Name>
      <Type>Alert</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Top</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <IncludeValidationSummary>false</IncludeValidationSummary>
    </Section>
    <Section xsi:type="ContentSection">
      <Name>Title</Name>
      <Type>Content</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Top</ContentPane>
      <SelectDataCommand>Worker_GetDashboardTitle</SelectDataCommand>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Content>
        &lt;h4 class="font-weight-bold py-3 mb-4"&gt;{WelcomeText}

        &lt;div class="text-muted text-tiny mt-1"&gt;
        &lt;small class="font-weight-normal"&gt;{CurrentDate}&lt;/small&gt;
        &lt;/div&gt;
        &lt;/h4&gt;
      </Content>
    </Section>
    <Section xsi:type="ContentSection">
      <ID>ContentSection</ID>
      <Name>Intro</Name>
      <Type>Content</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Left</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Content>&lt;div class="jumbotron"&gt;
              &lt;h1 class="display-3"&gt;Need Work?&lt;/h1&gt;
              &lt;p class="lead"&gt;Looking for relief pharmacy positions? We are here to help.&lt;/p&gt;
              &lt;hr class="my-4"&gt;
              
              &lt;p class="lead"&gt;
                &lt;a class="btn btn-success btn-lg" href="../Jobs/Search" role="button"&gt;Search for your Job Today&lt;/a&gt;
              &lt;/p&gt;
            &lt;/div&gt;
           </Content>
    </Section>
    <Section xsi:type="GridSection">
      <Name>Jobs you might be interested in</Name>
      <Type>Grid</Type>
      <Mode>All</Mode>
      <ContainerMode>Panel</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Right</ContentPane>
      <SelectDataCommand>Worker_GetJobMatches</SelectDataCommand>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <ActionLink>
        <ShowLink>false</ShowLink>
        <Permission>
          <CheckPermission>false</CheckPermission>
        </Permission>
      </ActionLink>
      <LoadDataOnPageLoad>true</LoadDataOnPageLoad>
      <Grid>
        <Name>Jobs you might be interested in</Name>
        <HelpText />
        <ShowHeader>true</ShowHeader>
        <CommandItemDisplay>None</CommandItemDisplay>
        <SelectDataCommand>Worker_GetJobMatches</SelectDataCommand>
        <AllowDelete>false</AllowDelete>
        <AllowPaging>true</AllowPaging>
        <PageSize>20</PageSize>
        <AllowSorting>true</AllowSorting>
        <SortOrder>Ascending</SortOrder>
        <Columns>
          <HyperLinkGridColumn>
            <HeaderStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </HeaderStyle>
            <ItemStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </ItemStyle>
            <FooterStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </FooterStyle>
            <HeaderText>Job Id</HeaderText>
            <ColumnType>HyperLinkGridColumn</ColumnType>
            <SortExpression>JobId</SortExpression>
            <VisiblePermission>
              <CheckPermission>false</CheckPermission>
            </VisiblePermission>
            <ExportPermission>
              <CheckPermission>false</CheckPermission>
            </ExportPermission>
            <LinkPermission>
              <CheckPermission>false</CheckPermission>
            </LinkPermission>
            <IncludeInExport>true</IncludeInExport>
            <DataNavigateUrlFields>JobGuid</DataNavigateUrlFields>
            <DataNavigateUrlFormatString>~/App/Jobs/Overview?JobGuid={0}</DataNavigateUrlFormatString>
            <DataTextField>JobId</DataTextField>
          </HyperLinkGridColumn>
          <BoundGridColumn>
            <HeaderStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </HeaderStyle>
            <ItemStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </ItemStyle>
            <FooterStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </FooterStyle>
            <HeaderText>Location</HeaderText>
            <ColumnType>BoundGridColumn</ColumnType>
            <SortExpression>LocationName</SortExpression>
            <VisiblePermission>
              <CheckPermission>false</CheckPermission>
            </VisiblePermission>
            <ExportPermission>
              <CheckPermission>false</CheckPermission>
            </ExportPermission>
            <LinkPermission>
              <CheckPermission>false</CheckPermission>
            </LinkPermission>
            <IncludeInExport>true</IncludeInExport>
            <DataField>LocationName</DataField>
          </BoundGridColumn>
          <BoundGridColumn>
            <HeaderStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </HeaderStyle>
            <ItemStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </ItemStyle>
            <FooterStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </FooterStyle>
            <HeaderText>Hourly Rate</HeaderText>
            <ColumnType>BoundGridColumn</ColumnType>
            <SortExpression>StaffRate</SortExpression>
            <VisiblePermission>
              <CheckPermission>false</CheckPermission>
            </VisiblePermission>
            <ExportPermission>
              <CheckPermission>false</CheckPermission>
            </ExportPermission>
            <LinkPermission>
              <CheckPermission>false</CheckPermission>
            </LinkPermission>
            <IncludeInExport>true</IncludeInExport>
            <DataField>StaffRate</DataField>
            <DataFormatString>{0:C}</DataFormatString>
          </BoundGridColumn>
          <BoundGridColumn>
            <HeaderStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </HeaderStyle>
            <ItemStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </ItemStyle>
            <FooterStyle>
              <CssClass />
              <HorizonalAlign>NotSet</HorizonalAlign>
              <VerticalAlign>NotSet</VerticalAlign>
              <Wrap>true</Wrap>
            </FooterStyle>
            <HeaderText>Shifts</HeaderText>
            <ColumnType>BoundGridColumn</ColumnType>
            <SortExpression>MatchingShiftCount</SortExpression>
            <VisiblePermission>
              <CheckPermission>false</CheckPermission>
            </VisiblePermission>
            <ExportPermission>
              <CheckPermission>false</CheckPermission>
            </ExportPermission>
            <LinkPermission>
              <CheckPermission>false</CheckPermission>
            </LinkPermission>
            <IncludeInExport>true</IncludeInExport>
            <DataField>MatchingShiftCount</DataField>
            <DataFormatString>{0} shifts match your schedule</DataFormatString>
          </BoundGridColumn>
        </Columns>
        <GroupByExpressions />
        <GroupLoadMode>Server</GroupLoadMode>
        <GroupsDefaultExpanded>true</GroupsDefaultExpanded>
        <AllowMultiRowSelection>false</AllowMultiRowSelection>
        <GroupingEnabled>false</GroupingEnabled>
        <ShowGroupPanel>false</ShowGroupPanel>
        <AllowGroupExpandCollapse>false</AllowGroupExpandCollapse>
        <ShowRefreshButton>true</ShowRefreshButton>
        <ShowExportToCsvButton>true</ShowExportToCsvButton>
        <ShowExportToExcelButton>false</ShowExportToExcelButton>
        <ShowExportToPdfButton>false</ShowExportToPdfButton>
        <ShowExportToWordButton>false</ShowExportToWordButton>
        <ExportIgnorePaging>true</ExportIgnorePaging>
        <ExportOpenInNewWindow>true</ExportOpenInNewWindow>
        <ExportHideStructureColumns>true</ExportHideStructureColumns>
        <AllowScroll>false</AllowScroll>
        <UseStaticHeaders>false</UseStaticHeaders>
        <FrozenColumnsCount>0</FrozenColumnsCount>
        <SaveScrollPosition>true</SaveScrollPosition>
        <CsvEncloseDataWithQuotes>false</CsvEncloseDataWithQuotes>
        <CsvColumnDelimiter>Comma</CsvColumnDelimiter>
        <CsvRowDelimiter>NewLine</CsvRowDelimiter>
        <AllowRowsDragDrop>false</AllowRowsDragDrop>
        <AllowRowSelect>false</AllowRowSelect>
        <EnableDragToSelectRows>false</EnableDragToSelectRows>
      </Grid>
    </Section>
  </Sections>
  <SectionZoneLayout>Default</SectionZoneLayout>
  <SectionZoneLayoutMode>Static</SectionZoneLayoutMode>
  <Settings />
  <Scripts />
  <DataCommands>
    <DataCommand>
      <Name>Worker_GetDashboardTitle</Name>
      <Parameters>
        <Parameter>
          <Name>@ProfileId</Name>
          <InputType>User</InputType>
          <InputKey>ProfileId</InputKey>
        </Parameter>
        <Parameter>
          <Name>@IsFirstTime</Name>
          <InputType>Constant</InputType>
          <InputKey>True</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
    <DataCommand>
      <Name>Worker_GetJobMatches</Name>
      <Parameters>
        <Parameter>
          <Name>@ProfileId</Name>
          <InputType>User</InputType>
          <InputKey>ProfileId</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
  </DataCommands>
  <OnPageLoad>
    <Commands />
  </OnPageLoad>
  <OnPageInit>
    <Commands>
      <RenderPageSectionsCommand>
        <Name>RenderPageSections</Name>
        <Type>RenderPageSectionsCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <EntityInputType>QueryString</EntityInputType>
        <Mode>Simple</Mode>
      </RenderPageSectionsCommand>
    </Commands>
  </OnPageInit>
</Screen>