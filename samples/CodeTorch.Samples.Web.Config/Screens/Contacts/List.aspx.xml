<?xml version="1.0" encoding="utf-8"?>
<Screen xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>List.aspx</Name>
  <PageTemplate>
    <Mode>Static</Mode>
    <Name>App</Name>
  </PageTemplate>
  <Type>Screen</Type>
  <Folder>Contacts</Folder>
  <RequireAuthentication>true</RequireAuthentication>
  <SupportsTransactions>true</SupportsTransactions>
  <ValidateRequest>true</ValidateRequest>
  <Menu>
    <Name />
    <DisplayMenu>false</DisplayMenu>
  </Menu>
  <Title>
    <Name>Contacts</Name>
    <UseCommand>false</UseCommand>
  </Title>
  <SubTitle>
    <Name />
    <UseCommand>false</UseCommand>
  </SubTitle>
  <ScreenPermission>
    <CheckPermission>false</CheckPermission>
  </ScreenPermission>
  <Sections>
    <Section xsi:type="AlertSection">
      <Name>Alert</Name>
      <Type>Alert</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Top</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <IncludeValidationSummary>false</IncludeValidationSummary>
    </Section>
    <Section xsi:type="ContentSection">
      <Name>IntroText</Name>
      <Type>Content</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Top</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Content>&lt;p style="padding-left: 2rem !important;"&gt;Below is a listing of people you have worked with in the past. You can contact them and book them as needed.&lt;/p&gt;</Content>
    </Section>
    <Section xsi:type="CustomSection">
      <Name>Chat</Name>
      <Type>Custom</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Content</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <UserControlPath>~/UserControls/ContactsChat.ascx</UserControlPath>
    </Section>
  </Sections>
  <SectionZoneLayout>SingleColumnChat</SectionZoneLayout>
  <SectionZoneLayoutMode>Static</SectionZoneLayoutMode>
  <Settings />
  <Scripts>
    <Script>
      <RenderAtTopOfPage>true</RenderAtTopOfPage>
      <IsOnSubmitScript>false</IsOnSubmitScript>
      <Path>https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js</Path>
    </Script>
  </Scripts>
  <DataCommands />
  <OnPageLoad>
    <Commands />
  </OnPageLoad>
  <OnPageInit>
    <Commands>
      <RenderPageSectionsCommand>
        <Name>RenderPageSections</Name>
        <Type>RenderPageSectionsCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <EntityInputType>QueryString</EntityInputType>
        <Mode>Simple</Mode>
      </RenderPageSectionsCommand>
    </Commands>
  </OnPageInit>
</Screen>