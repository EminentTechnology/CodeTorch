    <Section xsi:type="ContentSection">
      <Name>Actions</Name>
      <Type>Content</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Side</ContentPane>
      <SelectDataCommand>Profile_GetActions</SelectDataCommand>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Content>{ActionsHtml}</Content>
