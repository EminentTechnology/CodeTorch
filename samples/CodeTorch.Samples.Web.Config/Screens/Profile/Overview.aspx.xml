  <SectionZoneLayout>SingleColumnActions</SectionZoneLayout>
  <SectionZoneLayoutMode>Static</SectionZoneLayoutMode>
  <Settings />
  <Scripts />
  <DataCommands>
    <DataCommand>
      <Name>Profile_GetById</Name>
      <Parameters>
        <Parameter>
          <Name>@ProfileId</Name>
          <InputType>User</InputType>
          <InputKey>ProfileId</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
    <DataCommand>
      <Name>Profile_GetActions</Name>
      <Parameters>
        <Parameter>
          <Name>@ProfileId</Name>
          <InputType>User</InputType>
          <InputKey>ProfileId</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
    <DataCommand>
      <Name>Profile_GetOverviewScreenState</Name>
      <Parameters>
        <Parameter>
          <Name>@ProfileId</Name>
          <InputType>User</InputType>
          <InputKey>ProfileId</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
  </DataCommands>
  <OnPageLoad>
    <Commands>
      <DefaultOrPopulateScreenCommand>
        <Name>DefaultOrPopulateScreen</Name>
        <Type>DefaultOrPopulateScreenCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <EntityID>ProfileId</EntityID>
        <EntityInputType>User</EntityInputType>
        <RetrieveCommand>Profile_GetById</RetrieveCommand>
        <ExecuteOnPostBack>false</ExecuteOnPostBack>
      </DefaultOrPopulateScreenCommand>
      <ExecuteDataCommand>
        <Name>DisplayConfirmationMessages</Name>
        <Type>ExecuteDataCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <DataCommand>Worker_ProfileOverview_Messages</DataCommand>
      </ExecuteDataCommand>
    </Commands>
  </OnPageLoad>
  <OnPageInit>
    <Commands>
      <RenderPageSectionsCommand>
        <Name>RenderPageSections</Name>
        <Type>RenderPageSectionsCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <EntityInputType>QueryString</EntityInputType>
        <Mode>Simple</Mode>
      </RenderPageSectionsCommand>
    </Commands>
  </OnPageInit>
