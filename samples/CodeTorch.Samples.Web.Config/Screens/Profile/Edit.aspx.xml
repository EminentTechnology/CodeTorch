<?xml version="1.0" encoding="utf-8"?>
<Screen xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>Edit.aspx</Name>
  <PageTemplate>
    <Mode>Static</Mode>
    <Name>App</Name>
  </PageTemplate>
  <Type>Screen</Type>
  <Folder>Profile</Folder>
  <RequireAuthentication>true</RequireAuthentication>
  <SupportsTransactions>true</SupportsTransactions>
  <ValidateRequest>true</ValidateRequest>
  <Menu>
    <DisplayMenu>false</DisplayMenu>
  </Menu>
  <Title>
    <Name>My Profile</Name>
    <UseCommand>false</UseCommand>
  </Title>
  <SubTitle>
    <UseCommand>false</UseCommand>
  </SubTitle>
  <ScreenPermission>
    <CheckPermission>false</CheckPermission>
  </ScreenPermission>
  <Sections>
    <Section xsi:type="AlertSection">
      <Name>Alert</Name>
      <Type>Alert</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Top</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <IncludeValidationSummary>true</IncludeValidationSummary>
    </Section>
    <Section xsi:type="ContentSection">
      <Name>TitleArea</Name>
      <Type>Content</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Top</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Content>&lt;h4 class="font-weight-bold py-3 mb-4"&gt;Edit Profile &lt;/h4&gt;</Content>
    </Section>
    <Section xsi:type="EditSection">
      <Name>Details</Name>
      <Type>Edit</Type>
      <Mode>All</Mode>
      <ContainerMode>Plain</ContainerMode>
      <Visible>true</Visible>
      <ContainerElement>fieldset</ContainerElement>
      <ContainerCssClass>form-horizontal</ContainerCssClass>
      <ContentPane>Content</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets>
        <Widget xsi:type="TextBoxControl">
          <Name>FirstName</Name>
          <RenderLabel>true</RenderLabel>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-sm-10</ControlContainerCssClass>
          <Label>First Name</Label>
          <LabelCssClass>col-form-label col-sm-2 text-sm-right</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>form-group row</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>TextBox</Type>
          <IsRequired>true</IsRequired>
          <Visible>true</Visible>
          <DataField>FirstName</DataField>
          <Width>100%</Width>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
        </Widget>
        <Widget xsi:type="TextBoxControl">
          <Name>LastName</Name>
          <RenderLabel>true</RenderLabel>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-sm-10</ControlContainerCssClass>
          <Label>Last Name</Label>
          <LabelCssClass>col-form-label col-sm-2 text-sm-right</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>form-group row</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>TextBox</Type>
          <IsRequired>true</IsRequired>
          <Visible>true</Visible>
          <DataField>LastName</DataField>
          <Width>100%</Width>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
        </Widget>
        <Widget xsi:type="TextBoxControl">
          <Name>EmailAddress</Name>
          <RenderLabel>true</RenderLabel>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-sm-10</ControlContainerCssClass>
          <Label>Email Address</Label>
          <LabelCssClass>col-form-label col-sm-2 text-sm-right</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>form-group row</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>TextBox</Type>
          <IsRequired>true</IsRequired>
          <Visible>true</Visible>
          <DataField>EmailAddress</DataField>
          <Width>100%</Width>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
        </Widget>
        <Widget xsi:type="TextBoxControl">
          <Name>WorkPhone</Name>
          <RenderLabel>true</RenderLabel>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-sm-10</ControlContainerCssClass>
          <Label>Work Phone</Label>
          <LabelCssClass>col-form-label col-sm-2 text-sm-right</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>form-group row</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>TextBox</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <DataField>WorkPhone</DataField>
          <Width>100%</Width>
          <Validators>
            <RegularExpressionValidator>
              <Name>WorkPhoneRegularExpressionValidator</Name>
              <ErrorMessage>Enter work phone in the following format: 800-555-1212 - no extensions please</ErrorMessage>
              <ValidationExpression>^\D?(\d{3})\D?\D?(\d{3})\D?(\d{4})$</ValidationExpression>
            </RegularExpressionValidator>
          </Validators>
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
        </Widget>
        <Widget xsi:type="TextBoxControl">
          <Name>MobilePhone</Name>
          <RenderLabel>true</RenderLabel>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-sm-10</ControlContainerCssClass>
          <Label>Mobile Phone</Label>
          <LabelCssClass>col-form-label col-sm-2 text-sm-right</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>form-group row</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>TextBox</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <DataField>MobilePhone</DataField>
          <Width>100%</Width>
          <Validators>
            <RegularExpressionValidator>
              <Name>MobilePhoneRegularExpressionValidator</Name>
              <ErrorMessage>Enter mobile phone in the following format: 800-555-1212</ErrorMessage>
              <ValidationExpression>^\D?(\d{3})\D?\D?(\d{3})\D?(\d{4})$</ValidationExpression>
            </RegularExpressionValidator>
          </Validators>
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
        </Widget>
        <Widget xsi:type="CheckBoxControl">
          <Name>ReceiveSMS</Name>
          <RenderLabel>true</RenderLabel>
          <ControlCssClass />
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-sm-10</ControlContainerCssClass>
          <LabelCssClass>col-form-label col-sm-2 text-sm-right</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <LabelContainerElement />
          <LabelContainerCssClass />
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>form-group row</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>CheckBox</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <DataField>ReceiveSMS</DataField>
          <CssClass />
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
          <CheckedValue>True</CheckedValue>
          <UncheckedValue>False</UncheckedValue>
          <Text>Receive SMS Notifications</Text>
        </Widget>
      </Widgets>
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
    </Section>
    <Section xsi:type="ButtonListSection">
      <Name>Buttons</Name>
      <CssClass>text-center</CssClass>
      <Type>ButtonList</Type>
      <Mode>All</Mode>
      <ContainerMode>Plain</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Bottom</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Buttons>
        <Button>
          <Name>Save</Name>
          <RenderLabel>true</RenderLabel>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <Type>Button</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <CssClass>btn btn-primary</CssClass>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
          <Text>Save</Text>
          <CausesValidation>true</CausesValidation>
          <OnClick>
            <Commands>
              <InsertUpdateSaveCommand>
                <Name>SaveCommand</Name>
                <Type>InsertUpdateSaveCommand</Type>
                <ContinueOnError>false</ContinueOnError>
                <EntityID>ProfileId</EntityID>
                <EntityInputType>User</EntityInputType>
                <UpdateCommand>Profile_Update</UpdateCommand>
                <AfterInsertConfirmationMessage>Record was added successfully</AfterInsertConfirmationMessage>
                <AfterUpdateConfirmationMessage>Record was updated successfully</AfterUpdateConfirmationMessage>
                <RedirectAfterInsert>true</RedirectAfterInsert>
                <RedirectAfterUpdate>true</RedirectAfterUpdate>
                <AfterUpdateRedirectUrl>~/App/Profile/Overview</AfterUpdateRedirectUrl>
              </InsertUpdateSaveCommand>
            </Commands>
          </OnClick>
        </Button>
        <Button>
          <Name>Cancel</Name>
          <RenderLabel>true</RenderLabel>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <Type>Button</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <CssClass>btn btn-default</CssClass>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
          <Text>Cancel</Text>
          <CausesValidation>false</CausesValidation>
          <OnClick>
            <Commands>
              <NavigateToUrlCommand>
                <Name>NavigateOnCancel</Name>
                <Type>NavigateToUrlCommand</Type>
                <ContinueOnError>false</ContinueOnError>
                <NavigateUrl>~/App/Profile/Overview</NavigateUrl>
                <EntityID>ProfileId</EntityID>
              </NavigateToUrlCommand>
            </Commands>
          </OnClick>
        </Button>
      </Buttons>
    </Section>
  </Sections>
  <SectionZoneLayout>SingleColumnNoActions</SectionZoneLayout>
  <SectionZoneLayoutMode>Static</SectionZoneLayoutMode>
  <Settings />
  <Scripts />
  <DataCommands>
    <DataCommand>
      <Name>Profile_GetById</Name>
      <Parameters>
        <Parameter>
          <Name>@ProfileId</Name>
          <InputType>User</InputType>
          <InputKey>ProfileId</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
    <DataCommand>
      <Name>Profile_Update</Name>
      <Parameters>
        <Parameter>
          <Name>@ProfileId</Name>
          <InputType>User</InputType>
          <InputKey>ProfileId</InputKey>
        </Parameter>
        <Parameter>
          <Name>@Persona</Name>
          <InputType>Constant</InputType>
          <InputKey>1</InputKey>
        </Parameter>
        <Parameter>
          <Name>@EmployerId</Name>
          <InputType>Special</InputType>
          <InputKey>Null</InputKey>
        </Parameter>
        <Parameter>
          <Name>@FirstName</Name>
          <InputType>Control</InputType>
          <InputKey>FirstName</InputKey>
        </Parameter>
        <Parameter>
          <Name>@LastName</Name>
          <InputType>Control</InputType>
          <InputKey>LastName</InputKey>
        </Parameter>
        <Parameter>
          <Name>@EmailAddress</Name>
          <InputType>Control</InputType>
          <InputKey>EmailAddress</InputKey>
        </Parameter>
        <Parameter>
          <Name>@MobilePhone</Name>
          <InputType>Control</InputType>
          <InputKey>MobilePhone</InputKey>
        </Parameter>
        <Parameter>
          <Name>@WorkPhone</Name>
          <InputType>Control</InputType>
          <InputKey>WorkPhone</InputKey>
        </Parameter>
        <Parameter>
          <Name>@ReceiveSMS</Name>
          <InputType>Control</InputType>
          <InputKey>ReceiveSMS</InputKey>
        </Parameter>
        <Parameter>
          <Name>@ReceivePush</Name>
          <InputType>Control</InputType>
          <InputKey>ReceivePush</InputKey>
        </Parameter>
        <Parameter>
          <Name>@TimeZoneId</Name>
          <InputType>Special</InputType>
          <InputKey>Null</InputKey>
        </Parameter>
        <Parameter>
          <Name>@PostalCode</Name>
          <InputType>Control</InputType>
          <InputKey>PostalCode</InputKey>
        </Parameter>
        <Parameter>
          <Name>@Status</Name>
          <InputType>Special</InputType>
          <InputKey>Null</InputKey>
        </Parameter>
        <Parameter>
          <Name>@ModifiedBy</Name>
          <InputType>User</InputType>
          <InputKey>EmailAddress</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
  </DataCommands>
  <OnPageLoad>
    <Commands>
      <DefaultOrPopulateScreenCommand>
        <Name>DefaultOrPopulateScreen</Name>
        <Type>DefaultOrPopulateScreenCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <EntityID>ProfileId</EntityID>
        <EntityInputType>User</EntityInputType>
        <RetrieveCommand>Profile_GetById</RetrieveCommand>
        <ExecuteOnPostBack>false</ExecuteOnPostBack>
      </DefaultOrPopulateScreenCommand>
    </Commands>
  </OnPageLoad>
  <OnPageInit>
    <Commands>
      <RenderPageSectionsCommand>
        <Name>RenderPageSections</Name>
        <Type>RenderPageSectionsCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <EntityID>ProfileId</EntityID>
        <EntityInputType>User</EntityInputType>
        <Mode>InsertEdit</Mode>
      </RenderPageSectionsCommand>
    </Commands>
  </OnPageInit>
</Screen>