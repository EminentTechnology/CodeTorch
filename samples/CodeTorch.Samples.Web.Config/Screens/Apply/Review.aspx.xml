<?xml version="1.0" encoding="utf-8"?>
<Screen xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>Review.aspx</Name>
  <PageTemplate>
    <Mode>Static</Mode>
    <Name>App</Name>
  </PageTemplate>
  <Type>Screen</Type>
  <Folder>Apply</Folder>
  <RequireAuthentication>true</RequireAuthentication>
  <SupportsTransactions>true</SupportsTransactions>
  <ValidateRequest>true</ValidateRequest>
  <Menu>
    <Name>MainMenu</Name>
    <DisplayMenu>true</DisplayMenu>
  </Menu>
  <Title>
    <UseCommand>true</UseCommand>
    <CommandFormatString>{PageTitle}</CommandFormatString>
    <CommandName>Worker_GetJobTitle</CommandName>
  </Title>
  <SubTitle>
    <UseCommand>false</UseCommand>
  </SubTitle>
  <ScreenPermission>
    <CheckPermission>false</CheckPermission>
  </ScreenPermission>
  <Sections>
    <Section xsi:type="AlertSection">
      <Name>Alert</Name>
      <Type>Alert</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Top</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <IncludeValidationSummary>true</IncludeValidationSummary>
    </Section>
    <Section xsi:type="ContentSection">
      <Name>Title</Name>
      <Type>Content</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Top</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Content>&lt;h4 class="font-weight-bold py-3 mb-4"&gt;Review your job application
    &lt;/h4&gt;</Content>
    </Section>
    <Section xsi:type="DetailsSection">
      <Name>Review your job</Name>
      <CssClass />
      <Type>Details</Type>
      <Mode>All</Mode>
      <ContainerMode>Plain</ContainerMode>
      <Visible>true</Visible>
      <ContainerElement />
      <ContainerCssClass />
      <ContentPane>Content</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets>
        <Widget xsi:type="LabelControl">
          <Name>JobId</Name>
          <RenderLabel>true</RenderLabel>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-md-9</ControlContainerCssClass>
          <Label>Job Id</Label>
          <LabelCssClass>col-md-3 text-muted</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>row mb-2</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>Label</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <DataField>JobId</DataField>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
        </Widget>
        <Widget xsi:type="LabelControl">
          <Name>ProviderTypeName</Name>
          <RenderLabel>true</RenderLabel>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-md-9 review-jobtype</ControlContainerCssClass>
          <Label>Job Type</Label>
          <LabelCssClass>col-md-3 text-muted</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>row mb-2</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>Label</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <DataField>ProviderTypeName</DataField>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
        </Widget>
        <Widget xsi:type="LabelControl">
          <Name>StaffRate</Name>
          <RenderLabel>true</RenderLabel>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-md-9  review-hourlyrate</ControlContainerCssClass>
          <Label>Hourly Rate</Label>
          <LabelCssClass>col-md-3 text-muted</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>row mb-2</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>Label</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <DataField>StaffRate</DataField>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
          <FormatString>{0:C}</FormatString>
        </Widget>
        <Widget xsi:type="LabelControl">
          <Name>ShiftCount</Name>
          <RenderLabel>true</RenderLabel>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-md-9  review-shifts</ControlContainerCssClass>
          <Label>Shifts/Hours</Label>
          <LabelCssClass>col-md-3 text-muted</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>row mb-2</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>Label</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <DataField>JobApplicationShiftsDisplayText</DataField>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
        </Widget>
        <Widget xsi:type="LabelControl">
          <Name>IntroText</Name>
          <RenderLabel>true</RenderLabel>
          <ControlContainerElement>div</ControlContainerElement>
          <ControlContainerCssClass>col-md-9  review-details</ControlContainerCssClass>
          <Label>Introductory Note</Label>
          <LabelCssClass>col-md-3 text-muted</LabelCssClass>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <ControlGroupElement>div</ControlGroupElement>
          <ControlGroupCssClass>row mb-2</ControlGroupCssClass>
          <HelpTextElement>span</HelpTextElement>
          <HelpTextCssClass>help-block</HelpTextCssClass>
          <Type>Label</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <DataField>IntroText</DataField>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
        </Widget>
      </Widgets>
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
    </Section>
    <Section xsi:type="CustomSection">
      <Name>ProfileSection</Name>
      <Type>Custom</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Footer</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <UserControlPath>~/UserControls/PublicProfile.ascx</UserControlPath>
    </Section>
    <Section xsi:type="ButtonListSection">
      <Name>Buttons</Name>
      <CssClass>demo-inline-spacing d-flex justify-content-center</CssClass>
      <Type>ButtonList</Type>
      <Mode>All</Mode>
      <ContainerMode>Plain</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Bottom</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Buttons>
        <Button>
          <Name>SubmitJobApplication</Name>
          <RenderLabel>true</RenderLabel>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <Type>Button</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <CssClass>btn btn-primary</CssClass>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
          <Text>Submit Job Application</Text>
          <CausesValidation>false</CausesValidation>
          <OnClick>
            <Commands>
              <ExecuteDataCommand>
                <Name>SubmitJob</Name>
                <Type>ExecuteDataCommand</Type>
                <ContinueOnError>false</ContinueOnError>
                <DataCommand>Worker_SubmitJobApplication</DataCommand>
              </ExecuteDataCommand>
              <NavigateToUrlCommand>
                <Name>Confirmation</Name>
                <Type>NavigateToUrlCommand</Type>
                <ContinueOnError>false</ContinueOnError>
                <NavigateUrl>~/App/Apply/Confirmation</NavigateUrl>
                <Context>JobGuid</Context>
              </NavigateToUrlCommand>
            </Commands>
          </OnClick>
        </Button>
        <Button>
          <Name>Cancel</Name>
          <RenderLabel>true</RenderLabel>
          <LabelWrapsControl>false</LabelWrapsControl>
          <LabelRendersBeforeControl>true</LabelRendersBeforeControl>
          <Type>Button</Type>
          <IsRequired>false</IsRequired>
          <Visible>true</Visible>
          <CssClass>btn btn-default</CssClass>
          <Validators />
          <VisiblePermission>
            <CheckPermission>false</CheckPermission>
          </VisiblePermission>
          <EditPermission>
            <CheckPermission>false</CheckPermission>
          </EditPermission>
          <Text>Cancel - Go to Job Details</Text>
          <CausesValidation>false</CausesValidation>
          <OnClick>
            <Commands>
              <NavigateToUrlCommand>
                <Name>GoToJobs</Name>
                <Type>NavigateToUrlCommand</Type>
                <ContinueOnError>false</ContinueOnError>
                <NavigateUrl>~/App/Jobs/Overview</NavigateUrl>
                <Context>JobGuid</Context>
              </NavigateToUrlCommand>
            </Commands>
          </OnClick>
        </Button>
      </Buttons>
    </Section>
  </Sections>
  <SectionZoneLayout>CardWithFooter</SectionZoneLayout>
  <SectionZoneLayoutMode>Static</SectionZoneLayoutMode>
  <Settings />
  <Scripts>
    <Script>
      <RenderAtTopOfPage>true</RenderAtTopOfPage>
      <IsOnSubmitScript>false</IsOnSubmitScript>
      <Path>~/assets/js/app_apply_review.js</Path>
    </Script>
  </Scripts>
  <DataCommands>
    <DataCommand>
      <Name>Worker_GetJobTitle</Name>
      <Parameters>
        <Parameter>
          <Name>@ProfileId</Name>
          <InputType>User</InputType>
          <InputKey>ProfileId</InputKey>
        </Parameter>
        <Parameter>
          <Name>@JobGuid</Name>
          <InputType>QueryString</InputType>
          <InputKey>JobGuid</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
    <DataCommand>
      <Name>Worker_GetJobByGuid</Name>
      <Parameters>
        <Parameter>
          <Name>@ProfileId</Name>
          <InputType>User</InputType>
          <InputKey>ProfileId</InputKey>
        </Parameter>
        <Parameter>
          <Name>@JobGuid</Name>
          <InputType>QueryString</InputType>
          <InputKey>JobGuid</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
    <DataCommand>
      <Name>Worker_SubmitJobApplication</Name>
      <Parameters>
        <Parameter>
          <Name>@ProfileId</Name>
          <InputType>User</InputType>
          <InputKey>ProfileId</InputKey>
        </Parameter>
        <Parameter>
          <Name>@JobGuid</Name>
          <InputType>QueryString</InputType>
          <InputKey>JobGuid</InputKey>
        </Parameter>
        <Parameter>
          <Name>@ModifiedBy</Name>
          <InputType>User</InputType>
          <InputKey>EmailAddress</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
  </DataCommands>
  <OnPageLoad>
    <Commands>
      <DefaultOrPopulateScreenCommand>
        <Name>DefaultOrPopulateScreen</Name>
        <Type>DefaultOrPopulateScreenCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <EntityID>JobGuid</EntityID>
        <EntityInputType>QueryString</EntityInputType>
        <RetrieveCommand>Worker_GetJobByGuid</RetrieveCommand>
        <ExecuteOnPostBack>false</ExecuteOnPostBack>
      </DefaultOrPopulateScreenCommand>
    </Commands>
  </OnPageLoad>
  <OnPageInit>
    <Commands>
      <RenderPageSectionsCommand>
        <Name>RenderPageSections</Name>
        <Type>RenderPageSectionsCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <EntityID>JobGuid</EntityID>
        <EntityInputType>QueryString</EntityInputType>
        <Mode>InsertEdit</Mode>
      </RenderPageSectionsCommand>
    </Commands>
  </OnPageInit>
</Screen>