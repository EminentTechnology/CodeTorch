<?xml version="1.0" encoding="utf-8"?>
<Screen xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>RedirectToEmployerLogin.aspx</Name>
  <PageTemplate>
    <Mode>Static</Mode>
    <Name>Default</Name>
  </PageTemplate>
  <Type>Screen</Type>
  <Folder>Security</Folder>
  <RequireAuthentication>false</RequireAuthentication>
  <SupportsTransactions>true</SupportsTransactions>
  <ValidateRequest>true</ValidateRequest>
  <Menu>
    <DisplayMenu>false</DisplayMenu>
  </Menu>
  <Title>
    <UseCommand>false</UseCommand>
  </Title>
  <SubTitle>
    <UseCommand>false</UseCommand>
  </SubTitle>
  <ScreenPermission>
    <CheckPermission>false</CheckPermission>
  </ScreenPermission>
  <Sections />
  <SectionZoneLayoutMode>Static</SectionZoneLayoutMode>
  <Settings />
  <Scripts />
  <DataCommands>
    <DataCommand>
      <Name>Lookup_GetItem</Name>
      <Parameters>
        <Parameter>
          <Name>@LookupType</Name>
          <InputType>Constant</InputType>
          <InputKey>CONFIG</InputKey>
        </Parameter>
        <Parameter>
          <Name>@LookupValue</Name>
          <InputType>Constant</InputType>
          <InputKey>EmployerWebsiteAppUrl</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
  </DataCommands>
  <OnPageLoad>
    <Commands>
      <RedirectCommand>
        <Name>Redirect</Name>
        <Type>RedirectCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <DataCommand>Lookup_GetItem</DataCommand>
        <RedirectUrlField>LookupDescription</RedirectUrlField>
        <RedirectMode>DataCommand</RedirectMode>
      </RedirectCommand>
    </Commands>
  </OnPageLoad>
  <OnPageInit>
    <Commands />
  </OnPageInit>
</Screen>