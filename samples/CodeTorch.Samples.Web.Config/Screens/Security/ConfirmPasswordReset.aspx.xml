<?xml version="1.0" encoding="utf-8"?>
<Screen xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>ConfirmPasswordReset.aspx</Name>
  <PageTemplate>
    <Mode>Static</Mode>
    <Name>Default</Name>
  </PageTemplate>
  <Type>Screen</Type>
  <Folder>Security</Folder>
  <RequireAuthentication>false</RequireAuthentication>
  <SupportsTransactions>true</SupportsTransactions>
  <ValidateRequest>true</ValidateRequest>
  <Menu>
    <DisplayMenu>false</DisplayMenu>
  </Menu>
  <Title>
    <UseCommand>false</UseCommand>
  </Title>
  <SubTitle>
    <UseCommand>false</UseCommand>
  </SubTitle>
  <ScreenPermission>
    <CheckPermission>false</CheckPermission>
  </ScreenPermission>
  <Sections />
  <SectionZoneLayout>Default</SectionZoneLayout>
  <SectionZoneLayoutMode>Static</SectionZoneLayoutMode>
  <Settings />
  <Scripts />
  <DataCommands>
    <DataCommand>
      <Name>Profile_ResetPassword</Name>
      <Parameters>
        <Parameter>
          <Name>@TempPasswordActivationId</Name>
          <InputType>QueryString</InputType>
          <InputKey>ActivationId</InputKey>
        </Parameter>
        <Parameter>
          <Name>@Persona</Name>
          <InputType>Constant</InputType>
          <InputKey>2</InputKey>
        </Parameter>
      </Parameters>
    </DataCommand>
  </DataCommands>
  <OnPageLoad>
    <Commands>
      <RedirectCommand>
        <Name>Redirect</Name>
        <Type>RedirectCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <DataCommand>Profile_ResetPassword</DataCommand>
        <RedirectUrl>~/App/Security/ConfirmPasswordResetSuccess.aspx</RedirectUrl>
        <OnErrorRedirectUrl>~/App/Security/ConfirmPasswordResetFailed.aspx</OnErrorRedirectUrl>
        <RedirectMode>Constant</RedirectMode>
      </RedirectCommand>
    </Commands>
  </OnPageLoad>
  <OnPageInit>
    <Commands>
      <RenderPageSectionsCommand>
        <Name>RenderPageSections</Name>
        <Type>RenderPageSectionsCommand</Type>
        <ContinueOnError>false</ContinueOnError>
        <EntityInputType>QueryString</EntityInputType>
        <Mode>Simple</Mode>
      </RenderPageSectionsCommand>
    </Commands>
  </OnPageInit>
</Screen>