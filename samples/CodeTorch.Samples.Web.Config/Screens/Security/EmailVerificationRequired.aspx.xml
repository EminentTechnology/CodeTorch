<?xml version="1.0" encoding="utf-8"?>
<Screen xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>EmailVerificationRequired.aspx</Name>
  <PageTemplate>
    <Mode>Static</Mode>
    <Name>Signup</Name>
  </PageTemplate>
  <Type>Screen</Type>
  <Folder>Security</Folder>
  <RequireAuthentication>true</RequireAuthentication>
  <SupportsTransactions>true</SupportsTransactions>
  <ValidateRequest>true</ValidateRequest>
  <Menu>
    <DisplayMenu>false</DisplayMenu>
  </Menu>
  <Title>
    <Name>Email Verification Required</Name>
    <UseCommand>false</UseCommand>
  </Title>
  <SubTitle>
    <UseCommand>false</UseCommand>
  </SubTitle>
  <ScreenPermission>
    <CheckPermission>false</CheckPermission>
  </ScreenPermission>
  <Sections>
    <Section xsi:type="AlertSection">
      <Name>Alert</Name>
      <Type>Alert</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Content</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <IncludeValidationSummary>false</IncludeValidationSummary>
    </Section>
    <Section xsi:type="ContentSection">
      <ID>ContentSection</ID>
      <Name>Content</Name>
      <Type>Content</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Content</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Content>&lt;div class="display-1 lnr lnr-cross-circle text-center text-warning mb-4"&gt;&lt;/div&gt;

          &lt;p class="text-center text-big mb-4"&gt;Your Email Account needs to be verified!&lt;/p&gt;

          &lt;div class="alert alert-success"&gt;
            &lt;p class="small"&gt;Please check your email and click the &lt;b&gt;Activate Account&lt;/b&gt; link in the email.&lt;/p&gt;

            &lt;p class="small"&gt;Once your account is activated, you will be able to login to use RxTemps.&lt;/p&gt;
          &lt;/div&gt;</Content>
    </Section>
    <Section xsi:type="ContentSection">
      <Name>Resend</Name>
      <Type>Content</Type>
      <Mode>All</Mode>
      <ContainerMode>None</ContainerMode>
      <Visible>true</Visible>
      <ContentPane>Footer</ContentPane>
      <Permission>
        <CheckPermission>false</CheckPermission>
      </Permission>
      <Widgets />
      <AfterPopulateSection>
        <Commands />
      </AfterPopulateSection>
      <Content>&lt;div class="text-center text-muted"&gt;
            Need your activation email resent?
            &lt;a href="../Security/ResendActivationEmail"&gt;Resend&lt;/a&gt;
          &lt;/div&gt;</Content>
    </Section>
  </Sections>
  <SectionZoneLayout>Signup</SectionZoneLayout>
  <Settings />
  <Scripts />
  <DataCommands />
  <OnPageLoad>
    <Commands />
  </OnPageLoad>
  <OnPageInit>
    <Commands>
      <RenderPageSectionsCommand>
        <Name>RenderPageSections</Name>
        <Type>RenderPageSectionsCommand</Type>
        <EntityInputType>QueryString</EntityInputType>
        <Mode>Simple</Mode>
      </RenderPageSectionsCommand>
    </Commands>
  </OnPageInit>
</Screen>